<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <LangVersion>preview</LangVersion>
        <PackageId>FsJs</PackageId>
        <Authors>fc1943s</Authors>
        <RepositoryType>git</RepositoryType>
        <RepositoryUrl>https://github.com/fc1943s/FsJs</RepositoryUrl>
        <Description>Minimal JavaScript compatible F# bindings and utilities</Description>
        <Version>0.0.1</Version>
        <PackageVersion>0.0.1</PackageVersion>
        <PackageTags>fsharp;fable</PackageTags>
        <IncludeSymbols>true</IncludeSymbols>
        <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
    </PropertyGroup>
    <ItemGroup>
        <Compile Include="src\Core.fs" />
        <Compile Include="src\Js.fs" />
        <Compile Include="src\Bindings\Cypress.fs" />
        <Compile Include="src\Dom.fs" />
        <Compile Include="src\Profiling.fs" />
        <Content Include="paket.references" />
    </ItemGroup>
    <ItemGroup>
      <ProjectReference Include="..\FsCore\FsCore.fsproj" />
    </ItemGroup>
    <Import Project="..\..\.paket\Paket.Restore.targets" />
</Project>
